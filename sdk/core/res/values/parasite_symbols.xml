<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- The list of vendor package signatures that should also be considered
         platform signatures, specifically for use on devices with a vendor partition. -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- The list of Google platform package signatures
         that should also be considered platform signatures. -->
    <java-symbol type="array" name="config_googlePlatformSignatures" />
    <java-symbol type="array" name="config_googlePlatformSignaturesExtra" />

    <!-- The list of Sony platform package signatures
         that should also be considered platform signatures. -->
    <java-symbol type="array" name="config_sonyPlatformSignatures" />
    <java-symbol type="array" name="config_sonyPlatformSignaturesExtra" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Define device configs on boot -->
    <java-symbol type="array" name="global_device_configs_override" />
    <java-symbol type="array" name="device_configs_override" />

    <!-- Define phenotype/gservices flags -->
    <java-symbol type="array" name="global_phenotype_flags_override" />
    <java-symbol type="array" name="device_phenotype_flags_override" />
    <!-- Additional namespaces for packages' phenotype/gservices flags -->
    <java-symbol type="array" name="global_phenotype_package_namespaces" />
    <java-symbol type="array" name="device_phenotype_package_namespaces" />

    <!-- Prop imitation hook -->
    <java-symbol type="array" name="config_piHookPropsPixelXL" />
    <java-symbol type="array" name="config_piHookPropsPixelLegacy" />
    <java-symbol type="array" name="config_piHookProcessPixelLegacy" />
    <java-symbol type="array" name="config_piHookPropsPixelTablet" />
    <java-symbol type="array" name="config_piHookPropsPixelExtra" />
    <java-symbol type="array" name="config_piHookProcessPixelExtra" />
    <java-symbol type="array" name="config_piHookPropsPixelRecent" />
    <java-symbol type="array" name="config_piHookProcessPixelRecent" />
    <java-symbol type="array" name="config_piHookProcessKeep" />

    <!-- Custom features hook -->
    <java-symbol type="array" name="config_cfHookFeaturesPixel" />
    <java-symbol type="array" name="config_cfHookFeaturesPixelOthers" />
    <java-symbol type="array" name="config_cfHookFeaturesPixelTensor" />
    <java-symbol type="array" name="config_cfHookPackagesFeaturesPixelTensor" />
    <java-symbol type="array" name="config_cfHookFeaturesNexus" />
    <java-symbol type="array" name="config_cfHookFeaturesAndroid" />

    <!-- Game hook -->
    <java-symbol type="array" name="config_gameHook" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Whether device has a physical display cutout -->
    <java-symbol type="bool" name="config_physicalDisplayCutout" />

    <!-- Whether device has a big physical display cutout -->
    <java-symbol type="bool" name="config_physicalDisplayCutoutCentered" />

    <!-- Whitelist of package names to bypass foreground service type validation -->
    <java-symbol type="array" name="config_fgsTypeValidationBypassPackages"/>

    <!-- Whitelist of package names to bypass alarm manager validation -->
    <java-symbol type="array" name="config_alarmManagerValidationBypassPackages"/>

    <!-- Whitelist of package names to bypass broadcast reciever validation -->
    <java-symbol type="array" name="config_broadcastReceiverValidationBypassPackages"/>

    <!-- Whitelist of package names to bypass idle whitelist validation -->
    <java-symbol type="array" name="config_idleWhitelistValidationBypassPackages"/>

    <!-- Lock gesture -->
    <java-symbol type="string" name="gesture_locked_warning" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />
    <java-symbol type="array" name="config_cameraHFRPrivAppList" />

    <!-- These are system packages exempted from privacy indicator -->
    <java-symbol type="array" name="config_indicatorExemptedPackageNames" />

    <!-- PowerOffAlarmService -->
    <java-symbol type="bool" name="config_powerOffAlarmEnabled" />
    <java-symbol type="string" name="notification_channel_poweroff_alarm" />
    <java-symbol type="string" name="poweroff_alarm_title" />
    <java-symbol type="string" name="poweroff_alarm_body" />
    <java-symbol type="drawable" name="ic_alarm_on" />

    <!-- Display color inversion availability -->
    <java-symbol type="bool" name="config_displayInversionAvailable" />

    <!-- Cycle through ringer modes -->
    <java-symbol type="string" name="volume_dialog_ringer_guidance_normal" />

    <!-- Whether to use Richtap vibration -->
    <java-symbol type="bool" name="config_usesRichtapVibration" />

    <!-- Settings Restart -->
    <java-symbol type="string" name="settings_restart_title" />
    <java-symbol type="string" name="settings_restart_message" />

    <!-- Soft restart dialog -->
    <java-symbol type="string" name="system_restart_title" />
    <java-symbol type="string" name="system_restart_message" />

    <!-- SystemUI restart dialog -->
    <java-symbol type="string" name="systemui_restart_title" />
    <java-symbol type="string" name="systemui_restart_message" />

    <!-- Launcher restart dialog -->
    <java-symbol type="string" name="launcher_restart_title" />
    <java-symbol type="string" name="launcher_restart_message" />

    <!-- Whether to show ongoing progress bar chip in statusbar -->
    <java-symbol type="bool" name="config_enableOngoingProgressBarChip" />

    <!-- The list of packages to automatically opt out of FM Tuner input because
         of known compatibility issues. -->
    <java-symbol type="array" name="config_fmTunerInputBlacklist" />

    <!-- The list of packages to automatically opt out of FM output because
         of known compatibility issues. -->
    <java-symbol type="array" name="config_fmOutputBlacklist" />

</resources>
